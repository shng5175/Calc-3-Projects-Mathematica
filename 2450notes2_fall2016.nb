Cell["Polar Coordinates", "Section",
 CellChangeTimes->{{3.565012488221405*^9, 3.565012490675653*^9}}],

Cell["\<\
In chapter 9, we also introduced polar coordinates as an alternative way to \
describe points in the plane. In polar coordinates, we describe points via \
their angle with the positive x-axis, and the distance from the origin (don\
\[CloseCurlyQuote]t worry about the code for now, just look at the picture):\
\>", "Text",
 CellChangeTimes->{{3.565012510988823*^9, 3.565012577168343*^9}, {
  3.565012728886716*^9, 3.565012739551325*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"point", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"Black", ",", 
      RowBox[{"Disk", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"1", ",", "1"}], "}"}], ",", ".05"}], "]"}]}], "}"}], 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"liner", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"Red", ",", "Thick", ",", 
      RowBox[{"Line", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"0", ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"1", ",", "1"}], "}"}]}], "}"}], "]"}]}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"r", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"Text", "[", 
     RowBox[{
      RowBox[{"Style", "[", 
       RowBox[{"\"\<r\>\"", ",", "Large", ",", "Red"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{".5", ",", ".6"}], "}"}]}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"linex", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"Purple", ",", "Thick", ",", 
      RowBox[{"Line", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"0", ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"1", ",", "0"}], "}"}]}], "}"}], "]"}]}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"Text", "[", 
     RowBox[{
      RowBox[{"Style", "[", 
       RowBox[{"\"\<x\>\"", ",", "Large", ",", "Purple"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{".5", ",", 
        RowBox[{"-", ".1"}]}], "}"}]}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"liney", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"Orange", ",", "Thick", ",", 
      RowBox[{"Line", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"1", ",", "1"}], "}"}]}], "}"}], "]"}]}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"y", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"Text", "[", 
     RowBox[{
      RowBox[{"Style", "[", 
       RowBox[{"\"\<y\>\"", ",", "Large", ",", "Orange"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"1.1", ",", ".5"}], "}"}]}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"arc", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"Black", ",", "Thick", ",", 
      RowBox[{"Circle", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "0"}], "}"}], ",", ".2", ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{"\[Pi]", "/", "4"}]}], "}"}]}], "]"}]}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"theta", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"Text", "[", 
     RowBox[{
      RowBox[{"Style", "[", 
       RowBox[{"\"\<\[Theta]\>\"", ",", "Large"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{".3", ",", ".1"}], "}"}]}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Show", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "liner", ",", "r", ",", "linex", ",", "x", ",", "liney", ",", "y", ",", 
     "arc", ",", "theta", ",", "point"}], "}"}], ",", 
   RowBox[{"Axes", "\[Rule]", "True"}], ",", 
   RowBox[{"AxesStyle", "\[Rule]", "Gray"}], ",", 
   RowBox[{"AxesLabel", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Style", "[", 
       RowBox[{"\"\<x\>\"", ",", "Medium", ",", "Gray"}], "]"}], ",", 
      RowBox[{"Style", "[", 
       RowBox[{"\"\<y\>\"", ",", "Medium", ",", "Gray"}], "]"}]}], "}"}]}]}], 
  "]"}], "\[IndentingNewLine]"}], "Input",
 CellChangeTimes->{{3.5650126059136972`*^9, 3.565013130449942*^9}}],

Cell["\<\
From this picture, it should be clear that we can switch back and forth \
between the two coordinate systems in the following manner:\
\>", "Text",
 CellChangeTimes->{{3.565013188457304*^9, 3.56501321395649*^9}}],

Cell[BoxData[{
 RowBox[{"x", "=", 
  RowBox[{
   RowBox[{"r", " ", "cos", 
    RowBox[{"(", "\[Theta]", ")"}], "         ", "y"}], "=", 
   RowBox[{"r", " ", "sin", 
    RowBox[{"(", "\[Theta]", ")"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  SuperscriptBox["r", "2"], "=", 
  RowBox[{
   RowBox[{
    SuperscriptBox["x", "2"], "+", 
    RowBox[{
     SuperscriptBox["y", "2"], "             ", "tan", 
     RowBox[{"(", "\[Theta]", ")"}]}]}], "=", 
   FractionBox["y", "x"]}]}]}], "DisplayFormula",
 CellChangeTimes->{{3.5650132220844603`*^9, 3.565013261223694*^9}},
 TextAlignment->Center]
}, Open  ]]
}, Open  ]],

