Cell[CellGroupData[{

Cell["Polar Curves", "Section",
 CellChangeTimes->{{3.5650136009148283`*^9, 3.565013602511099*^9}}],

Cell[TextData[{
 "We can use polar coordinates to define curves, just like we use rectangular \
coordinates. Usually, when we need to plot a polar curve by hand, we make a \
table consisting of the radius that results at various angles. Fortunately, \
we can also use ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " to plot polar curves. Below are some examples of common polar curves:"
}], "Text",
 CellChangeTimes->{{3.565013605910768*^9, 3.565013716536734*^9}}],

Cell[BoxData[
 RowBox[{"PolarPlot", "[", 
  RowBox[{"3", ",", 
   RowBox[{"{", 
    RowBox[{"\[Theta]", ",", "0", ",", 
     RowBox[{"2", "\[Pi]"}]}], "}"}], ",", 
   RowBox[{"PlotStyle", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"Thick", ",", "Red"}], "}"}]}], ",", 
   RowBox[{"PlotLabel", "\[Rule]", 
    RowBox[{"Style", "[", 
     RowBox[{"\"\<r = 3\>\"", ",", "Red", ",", "Large"}], "]"}]}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.5650137206904793`*^9, 3.565013829948184*^9}}],

Cell[BoxData[
 RowBox[{"PolarPlot", "[", 
  RowBox[{
   RowBox[{"3", 
    RowBox[{"(", 
     RowBox[{"1", "-", 
      RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ")"}]}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Theta]", ",", "0", ",", 
     RowBox[{"2", "\[Pi]"}]}], "}"}], ",", 
   RowBox[{"PlotStyle", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"Thick", ",", "Orange"}], "}"}]}], ",", 
   RowBox[{"PlotLabel", "\[Rule]", 
    RowBox[{"Style", "[", 
     RowBox[{"\"\<r = 3(1-Cos[\[Theta]])\>\"", ",", "Orange", ",", "Large"}], 
     "]"}]}]}], "]"}]], "Input",
 CellChangeTimes->{{3.565013836784071*^9, 3.565013889763364*^9}}],

Cell[BoxData[
 RowBox[{"PolarPlot", "[", 
  RowBox[{
   RowBox[{"2", 
    RowBox[{"Cos", "[", 
     RowBox[{"4", "\[Theta]"}], "]"}]}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Theta]", ",", "0", ",", 
     RowBox[{"2", "\[Pi]"}]}], "}"}], ",", 
   RowBox[{"PlotStyle", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"Thick", ",", "Green"}], "}"}]}], ",", 
   RowBox[{"PlotLabel", "\[Rule]", 
    RowBox[{"Style", "[", 
